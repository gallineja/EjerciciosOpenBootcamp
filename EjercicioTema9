public class Main {
    public static void main(String[] args) {
         Cliente cliente = new Cliente();
         Trabajador trabajador = new Trabajador();
         cliente.edad = 25;
         cliente.nombre = "Norberto";
         cliente.telefono = "666555444";
         cliente.credito = 2000;
         System.out.println("Mi nombre es " + cliente.nombre + ", mi edad es " + cliente.edad + " y mi teléfono es " + cliente.telefono);
         System.out.println("Tengo de crédito: " + cliente.credito);
         trabajador.edad = 55;
         trabajador.nombre = "Julio";
         trabajador.telefono = "888333222";
         trabajador.salario = 3000;
         System.out.println("Mi nombre es " + trabajador.nombre + ", mi edad es " + trabajador.edad + " y mi teléfono es " + trabajador.telefono);
         System.out.println("Mi salario es: " + trabajador.salario);
    }
}
class Persona {
    int edad;
    String nombre;
    String telefono;
 
}
class Cliente extends Persona{
    int credito;
}
class Trabajador extends Persona{
    int salario;
}
